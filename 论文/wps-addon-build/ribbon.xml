<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="Paper_onload">
    <ribbon startFromScratch="false">
        <tabs>
            <tab id="wpsAddinTab" label="论文">
                <group id="Refer_grp" label="参考文献">
                    <button id="format" label="索引格式检查" onAction="CheckIndexFormat.step" enabled="true" imageMso="Spelling" size="large"/>
                    <button id="up_err" label="" onAction="CheckIndexFormat.step" enabled="true" imageMso="OutlineMoveUp" size="small"/>
                    <button id="down_err" label="" onAction="CheckIndexFormat.step" enabled="true" imageMso="OutlineMoveDown" size="small"/>
                    <button id="context" label="上下文核对" onAction="CheckContext.step" enabled="true" imageMso="FormControlCheckBox" size="large"/>
                </group>
                <group id="Result_grp" label="结果">
                    <button id="write" label="撰写结果" onAction="writeResult" enabled="true" imageMso="CharacterBorder" size="large"/>
                    <button id="link" label="引用" onAction="textLink.step" enabled="true" imageMso="Text" size="small"/>
                </group>
                <group id="Chart_grp" label="图表">
                    <button id="figure" label="作图" onAction="addFigure.step" enabled="true" imageMso="ChartInsert" size="small"/>
                    <button id="table" label="作表" onAction="addTable.step" enabled="true" imageMso="TableDesign" size="small"/>
                    <button id="flowChart" label="流程图" onAction="addFlowChart.step" enabled="false" imageMso="TracePrecedents" size="large"/>
                </group>
                <group  id="Other_grp" label="其他">
                    <button id="frame" label="创建框架" onAction="createFrame.step" enabled="false" imageMso="Zoom100" size="large"/>
                    <button id="identify" label="识别大纲" onAction="identifyOutline.step" enabled="true" imageMso="XmlStructure" size="small"/>
                    <button id="test" label="测试" onAction="test" enabled="false" imageMso="Text" size="small"/>
                    <!-- <button id="btnIsEnbable" label="全文" enabled="false" imageMso="Text" size="small"/> -->
                    <button id="TaskPane" label="帮助" onAction="ShowHelp" enabled="true" imageMso="Help" size="large"/>
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>